<%= form_for(@setting) do |f| %>
<div class="container">
  <div class="field"> 
    <%= f.label :country %>          
      <%= f.select :country, options_for_country, {}, {:prompt => 'Select One Country',  autofocus: 'true', class: 'form-control'}  %>
  </div> 
  <br>
  <div class="field">
    <%= f.label :language %>           
      <%= f.select :language, options_for_language, {}, {:prompt => 'Select One Country',  autofocus: 'true', class: 'form-control'}  %>
  </div>    
   
  <div class="field">
    <%= f.submit "Guardar", class:"btn btn-sm btn-danger white pull-right", style:"margin-top: 15px;" %>
  </div>
<% end %>
  </div>

